{
  "name": "umbraco-mcp-server-sdk-monorepo",
  "version": "17.0.0-beta.3",
  "private": true,
  "description": "Umbraco MCP Server SDK monorepo",
  "workspaces": [
    "packages/*",
    "template",
    "plugins"
  ],
  "scripts": {
    "build": "npm run build -w packages/mcp-server-sdk",
    "build:create": "npm run build -w packages/create-mcp-server",
    "build:hosted": "npm run build -w packages/hosted-mcp",
    "build:all": "npm run build && npm run build:hosted && npm run build:create",
    "clean": "npm run clean -w packages/mcp-server-sdk",
    "compile": "npm run compile -w packages/mcp-server-sdk",
    "test": "npm run test -w packages/mcp-server-sdk"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umbraco/Umbraco-MCP-Base.git"
  },
  "keywords": [
    "Umbraco",
    "MCP",
    "Model Context Protocol"
  ],
  "author": "Phil Whittaker",
  "license": "MIT"
}
