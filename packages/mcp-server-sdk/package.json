{
  "name": "@umbraco-cms/mcp-server-sdk",
  "version": "17.0.0-beta.1",
  "type": "module",
  "description": "Umbraco-specific MCP infrastructure and patterns for building MCP servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    },
    "./evals": {
      "types": "./dist/evals/index.d.ts",
      "import": "./dist/evals/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "compile": "tsc --noEmit",
    "watch": "tsup --watch",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umbraco/Umbraco-MCP-Base.git"
  },
  "keywords": [
    "Umbraco",
    "MCP",
    "Model Context Protocol",
    "sdk"
  ],
  "author": "Umbraco HQ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco-MCP-Base/issues"
  },
  "homepage": "https://github.com/umbraco/Umbraco-MCP-Base#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "axios": "^1.8.4",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.14.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-agent-sdk": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/claude-agent-sdk": {
      "optional": true
    }
  }
}
