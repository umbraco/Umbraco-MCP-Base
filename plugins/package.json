    "test:all": "node --experimental-vm-modules $(npm root)/jest/bin/jest.js --runInBand --testTimeout=120000 --forceExit"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.6.3"
  }
}
