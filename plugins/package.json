{
  "name": "@umbraco-cms/mcp-skills",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Skills and agents for building Umbraco MCP servers",
  "scripts": {
    "test": "node --experimental-vm-modules $(npm root)/jest/bin/jest.js --testPathPattern=__tests__ --runInBand --passWithNoTests",
